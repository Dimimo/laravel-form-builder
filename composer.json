{
  "name": "kris/laravel-form-builder",
  "description": "Laravel form builder - symfony like",
  "keywords": [
    "laravel",
    "form",
    "builder",
    "symfony"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Kristijan Husak",
      "email": "husakkristijan@gmail.com"
    },
    {
      "name": "Mike Erickson",
      "email": "codedungeon@gmail.com"
    },
    {
      "name": "Mike Erickson",
      "email": "mike.erickson@codedungeon.io"
    }
  ],
  "require": {
    "php": ">=7.1",
    "laravelcollective/html": "^6.2",
    "illuminate/database": "^7|^8",
    "illuminate/validation": "^7|^8",
    "ext-iconv": "*"
  },
  "require-dev": {
    "facade/ignition": "^2.3",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.4",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^8.5"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.16.x-dev"
    },
    "laravel": {
      "providers": [
        "Kris\\LaravelFormBuilder\\FormBuilderServiceProvider"
      ],
      "aliases": {
        "FormBuilder": "Kris\\LaravelFormBuilder\\Facades\\FormBuilder"
      }
    }
  },
  "autoload": {
    "psr-0": {
      "Kris\\LaravelFormBuilder": "src/"
    },
    "classmap": [
      "tests/FormBuilderTestCase.php"
    ],
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
